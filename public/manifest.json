{
  "manifest_version": 3,
  "name": "Scenic New Tab",
  "version": "1.5",
  "description": "Scenic",
  "author": "Varun Gaikwad",
  "permissions": ["tabs", "storage", "activeTab", "geolocation"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; frame-src https://www.youtube.com/"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png"
  }
}
